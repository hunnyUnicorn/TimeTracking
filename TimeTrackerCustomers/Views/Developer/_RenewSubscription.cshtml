@model RenewSub
@{
    var Projects = ViewData["Subcriptions"] as List<SelectListItem>;
}
<div class="modal-header">
    <h5 class="modal-title  fw-bold" id="createprojectlLabel"> Renew Subscription</h5>
    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
</div>
<div class="modal-body">
    <form id="SubscriptionForm">
        <div class="mb-3">
            <label asp-for="SubPlanCode" class="form-label">Subscription</label>
            <select asp-for="SubPlanCode" asp-items="Projects" class="form-select" aria-label="Default select Project">
                <option>Select Subscription</option>
            </select>
        </div>
        <div class="row">
            <label class="text-danger" id="errorMessage"></label>
        </div>
    </form>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
    <button type="submit" form="SubscriptionForm" class="btn btn-primary">Checkout</button>
</div>